{
  "name": "quangthinh-sitemap",
  "version": "1.0.0",
  "description": "Sitemap Generator",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "Sitemap",
    "Google",
    "News"
  ],
  "author": "quangthinh <quangthinh.dico@gmail.com>",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
